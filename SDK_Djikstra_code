/*
 * Empty C++ Application
 */
#define max_vertex 50     	/* It is the maximum number of vertices in the graph */
//#define INT_MAX 2147483647
#define INT_MAX __INT_MAX__

void dijkstra(unsigned int graph[max_vertex][max_vertex], int path[max_vertex], unsigned int src, unsigned int dstn)
{
	unsigned int dist[max_vertex];
	bool Dset[max_vertex];
	/*Initialize the distance of the source vertec to zero*/
	/* A method to initialize the vector values and source distance */
	for(unsigned int i = 0; i < max_vertex; i++)                    /* Initialize distance of all the vertex to INFINITY and Dset as false */
	{
		path[i] = -1;
		dist[i] = INT_MAX;
		Dset[i] = false;
	}
	dist[src] = 0;      // Set the distance from the source to the source to zero.


	for(unsigned int c = 0; c < max_vertex; c++)
	{
		/* u is the index of any vertex that is not yet included in Dset and has minimum distance */
		unsigned int min = INT_MAX;
		unsigned int index = INT_MAX;

	for(unsigned int z = 0; z < max_vertex; z++)
		{
			if( (Dset[z] == false) && (dist[z] <= min) )
			{
				min   = dist[z];
				index = z;
			}
		}
		if(index == dstn) break;
		Dset[index] = true;  /* If the vertex with minimum distance was found, include it to Dset */


	for(unsigned int v = 0; v < max_vertex; v++)
		{
			/*
			   Update dist[v] if not in Dset and their is a path from src to v through u
			   that has distance minimum than current value of dist[v]
			*/
			if( (!Dset[v] && graph[index][v]) && (dist[index] != INT_MAX) && (dist[index]+graph[index][v] < dist[v]) ) {
				dist[v] = dist[index] + graph[index][v];
				path[v] = index;
			}
		}
	}
	path[0] = 0;
	return;
}


